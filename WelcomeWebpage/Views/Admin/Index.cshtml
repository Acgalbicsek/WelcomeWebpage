@model IEnumerable<WelcomeWebpage.Models.AdminInfo>

<h2>Admin Login</h2>

<form method="post">
    <div>
        <label>Username:</label>
        <input type="text" name="Username" />
    </div>
    <div>
        <label>Password:</label>
        <input type="password" name="Password" />
    </div>
    <button type="submit">Login</button>
</form>




